{
    "name": "lionix/castable-request",
    "description": "Laravel castable request validation rule",
    "keywords": [
        "laravel",
        "request",
        "validation",
        "cast"
    ],
    "type": "library",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2.5",
        "laravel/framework": ">=7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^5.5",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Lionix\\CastableRequest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lionix\\CastableRequest\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lionix\\CastableRequest\\ServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "vaawebdev",
            "email": "vaawebdev@gmail.com"
        }
    ]
}