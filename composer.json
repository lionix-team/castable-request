{
    "name": "lionix/castable-request",
    "description": "Laravel castable requests",
    "keywords": [
        "laravel",
        "request",
        "cast"
    ],
    "license": "MIT",
    "type": "library",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2.5|^8.0",
        "laravel/framework": ">=6.20.12"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^4.8",
        "phpunit/phpunit": "^9.3.3",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Lionix\\CastableRequest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lionix\\CastableRequest\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit",
        "lint": "phpcs ./src"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lionix\\CastableRequest\\ServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "vaawebdev",
            "email": "vaawebdev@gmail.com"
        }
    ]
}
